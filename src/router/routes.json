[
  {
    "path": "/",
    "name": "home",
    "component": "HomeView",
    "alias": "/home",
    "meta": {
      "title": "首页",
      "keepAlive": true,
      "icon": "home"
    }
  },
  {
    "path": "/pinia-demo",
    "name": "pinia-demo",
    "component": "PiniaDemoView",
    "meta": {
      "title": "Pinia 示例",
      "requiresAuth": false
    }
  },
  {
    "path": "/serialport-demo",
    "name": "serialport-demo",
    "component": "SerialportView",
    "meta": {
      "title": "串口示例",
      "keepAlive": false
    }
  },
  {
    "path": "/:pathMatch(.*)*",
    "name": "not-found",
    "component": "NotFoundView",
    "meta": {
      "title": "页面不存在",
      "layout": "blank"
    }
  }
]
